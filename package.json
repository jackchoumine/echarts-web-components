    "g": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.13.0",
    "echarts": "~5.4.0",
    "element-resize-detector": "~1.2.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackchoumine/echart-web-components.git"
  },
  "homepage": "https://github.com/jackchoumine/echarts-web-components"
}
